{
  "expo": {
    "name": "aas-tracker",
    "slug": "ass-tracker-knit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/knit-logo.png",
    "scheme": "aas-tracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.abhayofc.asstrackerknit"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/knit-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9987f418-bc80-40e5-a521-9b65aed4389a"
      }
    }
  }
}